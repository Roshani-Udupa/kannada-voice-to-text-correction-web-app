{
  "name": "kannada-voice-to-text-correction",
  "version": "1.0.0",
  "description": "Kannada Voice-to-Text Correction Web Application",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "install-all": "npm install && cd client && npm install",
    "build": "cd client && npm run build"
  },
  "keywords": [
    "kannada",
    "speech-to-text",
    "transcription",
    "correction"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@elevenlabs/elevenlabs-js": "^2.24.1",
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "form-data": "^4.0.0",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.1"
  }
}
